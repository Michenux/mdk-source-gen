<configuration xmlns="http://www.sopragroup.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:schemaLocation="http://www.sopragroup.com ../xmlMerge.xsd">
	<node name="manifest"			identificationType="root" 													content="children"	/>
	<node name="application"		identificationType="name" 													content="children"	/>
	<node name="action" 			identificationType="parent+name+attr" 	idAttribute="android:name"			content="none"		/>
	<node name="activity"			identificationType="name+attr" 			idAttribute="android:name" 			content="children"	/>
	<node name="activity-alias"		identificationType="name+attr" 			idAttribute="android:name" 			content="children"	/>
	<node name="category" 			identificationType="parent+name+attr"	idAttribute="android:name"			content="none"		/>
	<node name="data" 				identificationType="parent+name+attr"	idAttribute="android:scheme|android:host|android:port|android:path|android:pathPattern|android:pathPrefix|android:mimeType"	content="none"/>
	<node name="grant-uri-permission" identificationType="parent+name"											content="none"		/>
	<node name="instrumentation" 	identificationType="name+attr"			idAttribute="android:name"			content="none"		/>
	<node name="intent-filter" 		identificationType="parent+name"											content="children"	/>
	<node name="meta-data" 			identificationType="parent+name"											content="none"		/>
	<node name="path-permission"	identificationType="parent+name"											content="none"		/>
	<node name="permission"			identificationType="name+attr"			idAttribute="android:name"			content="none"		/>
	<node name="permission-group"	identificationType="name+attr"			idAttribute="android:name"			content="none"		/>
	<node name="permission-tree"	identificationType="name+attr"			idAttribute="android:name"			content="none"		/>
	<node name="provider"			identificationType="name+attr"			idAttribute="android:name"			content="children"	/>
	<node name="receiver"			identificationType="name+attr"			idAttribute="android:name"			content="children"	/>
	<node name="service"			identificationType="name+attr"			idAttribute="android:name"			content="children"	/>
	<node name="supports-screens"	identificationType="name"													content="none"		/>
	<node name="uses-configuration"	identificationType="name"													content="none"		/>
	<node name="uses-feature"		identificationType="name+attr"			idAttribute="android:name"			content="none"		/>
	<node name="uses-library"		identificationType="name+attr"			idAttribute="android:name"			content="none"		/>
	<node name="uses-permission"	identificationType="name+attr"			idAttribute="android:name"			content="none"		/>
	<node name="uses-sdk"			identificationType="name"													content="none"		/>
</configuration>