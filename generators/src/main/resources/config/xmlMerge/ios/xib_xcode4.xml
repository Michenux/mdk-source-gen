<!-- This XML file is to open with the Eclipse plugin "Rinzo"-->
<configuration xmlns="http://www.sopragroup.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:schemaLocation="http://www.sopragroup.com ../xmlMerge.xsd">
	<node name="archive"						identificationType="root" 														content="children"	/>
	<node name="data"							identificationType="name"														content="children"	/>
	<node name="bool"							identificationType="parent+name+attr|value"		idAttribute="id|key"			content="text"	/>
	<node name="boolean"						identificationType="parent+name+attr|value"		idAttribute="id|key"			content="text"	/>
	<node name="int"							identificationType="parent+name+attr|value" 	idAttribute="id|key"			content="text"	/>
	<node name="integer"						identificationType="parent+name+attr|value" 	idAttribute="id|key"			content="text"	/>
	<node name="float"							identificationType="parent+name+attr|value" 	idAttribute="id|key"			content="text"	/>
	<node name="real"							identificationType="parent+name+attr|value" 	idAttribute="id|key"			content="text"	/>
	<node name="double"							identificationType="parent+name+attr|value" 	idAttribute="id|key"			content="text"	/>
	<node name="string"							identificationType="parent+name+attr|value"		idAttribute="id|key"			content="text"	/>
	<node name="bytes"							identificationType="parent+name+attr|value" 	idAttribute="id|key"			content="text"	/>
	<node name="nil"							identificationType="parent+name+attr|value" 	idAttribute="id|key"			content="none"	/>
	<node name="reference"						identificationType="parent+name+attr"		 	idAttribute="key"				content="none"	/>
	<node name="object"							identificationType="parent+name+attr" 			idAttribute="key"				content="children"	/>
	<node name="array"							identificationType="parent+name+attr" 			idAttribute="id"				content="children"	/>
</configuration>
