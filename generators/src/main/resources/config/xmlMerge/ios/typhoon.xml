<configuration xmlns="http://www.sopragroup.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:schemaLocation="http://www.sopragroup.com ../xmlMerge.xsd">
	<node name="assembly"						identificationType="root" 														content="children"	/>
	<node name="description"					identificationType="parent+name"												content="text"	/>
	<node name="component"						identificationType="parent+name+attr"			idAttribute="key|class"			content="children"	/>
	<node name="property"						identificationType="parent+name+attr"			idAttribute="name"				content="none"	/>
	<node name="initializer"					identificationType="parent+name+attr"			idAttribute="selector"			content="children"	/>
	<node name="argument"						identificationType="parent+name+attr"			idAttribute="parameterName"		content="none"	/>
</configuration>
