<configuration xmlns="http://www.sopragroup.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:schemaLocation="http://www.sopragroup.com ../xmlMerge.xsd">
	<node name="model"							identificationType="root" 														content="children"	/>
	<node name="entity"							identificationType="name+attr"					idAttribute="name"				content="children"	/>
	<node name="attribute"						identificationType="parent+name+attr"			idAttribute="name"				content="none"	/>
	<node name="relationship"					identificationType="parent+name+attr"			idAttribute="name"				content="none"	/>
	<node name="fetchedProperty"				identificationType="parent+name+attr"			idAttribute="name"				content="children"	/>
	<node name="fetchRequest"					identificationType="parent+name+attr"			idAttribute="name"				content="none"	/>
	<node name="configuration"					identificationType="name+attr"					idAttribute="name"				content="children"	/>
	<node name="memberEntity"					identificationType="parent+name+attr"			idAttribute="name"				content="none"	/>
	<node name="elements"						identificationType="name"														content="children"	/>
	<node name="element"						identificationType="name+attr"					idAttribute="name"				content="none"	/>
</configuration>
