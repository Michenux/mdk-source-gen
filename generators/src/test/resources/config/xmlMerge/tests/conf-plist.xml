<configuration xmlns="http://www.sopragroup.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:schemaLocation="http://www.sopragroup.com ../xmlMerge.xsd">
	<node name="plist"							identificationType="root" 												content="children"	/>
	<node name="dict"							identificationType="name+attr"				idAttribute="key" 			content="children"	/>
	<node name="dummy"							identificationType="keyForSibling"			idAttribute="key" 			content="children"	/>
	<node name="artificialGroupingForXmlMerge"	identificationType="name+attr"				idAttribute="key" 			content="children"	/>
	<node name="key"							identificationType="parent+name"										content="text"	/>
	<node name="string"							identificationType="parent+name"										content="text"	/>
	<node name="true"							identificationType="parent+name"										content="none"	/>
	<node name="false"							identificationType="parent+name"										content="none"	/>
	<node name="integer"						identificationType="parent+name"										content="text"	/>
	<node name="array"							identificationType="parent+name"										content="children"	/>
	<node name="data"							identificationType="parent+name"										content="text"	/>
	<node name="date"							identificationType="parent+name"										content="text"	/>
</configuration>
